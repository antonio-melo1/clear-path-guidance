SUPABASE_URL=https://your-project.supabase.co
# Use the Service Role Key on the server only (keeps RLS from blocking inserts)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
# Optional fallback: public anon key (not recommended for server inserts if RLS is enabled)
SUPABASE_API_KEY=your_anon_key_here

# URL that the frontend will call in development (no trailing slash)
VITE_API_URL=http://localhost:4000

# Server port
PORT=4000
